page {
  height: 100%;
}

.canvas_container {
  position: fixed;
  width: 100%;
  height: 100%;
}

.canvas_area {
  width: 100%;
  height: 90%;
  background-color: #f8f8f8;
}

.myCanvas {
  width: 100%;
  height: 100%;
}

.canvas_tools {
  background-color: #f8f8f8;
  position: fixed;
  left: 0;
  bottom: 0rpx;
  width: 100%;
  height: 10%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}

.box {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rebeccapurple;
}

